doctype html
html 
    head 
        title= title
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Dogs
        p Welcome to the Custom Dogs page!
        p Title: #{title}
        form (action="/update-cobj" method="post")
            label(for="name") Name:
            input(type="text" id="name" name="dogData[name]" required)
            br
            label(for="breed") Breed:
            input(type="text" id="breed" name="dogData[breed]" required)
            br
            label(for="age") Age:
            input(type="number" id="age" name="dogData[age]" required)
            button(type="submit") Update or Create a Dog
        p #{message}